{
    "name": "berkayk/laravel-cart",
    "description": "Laravel shopping cart management in an easy way.",
    "require": {
        "php": ">=8.0.0",
        "illuminate/support": "8.47.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.7.*",
        "mockery/mockery": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gaurav Makhecha",
            "email": "info@freshbits.in"
        },
        {
            "name": "Berkay Kaya",
            "email": "berkayk@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Freshbitsweb\\LaravelCartManager\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Freshbitsweb\\LaravelCartManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Freshbitsweb\\LaravelCartManager\\CartManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
